<div class="row">
    <div class="col-lg-8 col-lg-offset-2">
        <form class="form-horizontal">
            <div class="form-group">
                <label for="name" class="col-sm-2 control-label">Project Name</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="name" placeholder="Project name">
                </div>
            </div>
            <div class="form-group">
                <label for="description" class="col-sm-2 control-label">Description</label>
                <div class="col-sm-10">
                    <textarea class="form-control" rows="5" id="description"></textarea>
                </div>
            </div>
            <div class="form-group">
                <label for="image_1" class="col-sm-2 control-label">Image 1</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="image_1" placeholder="url">
                </div>
            </div>
            <div class="form-group">
                <label for="image_2" class="col-sm-2 control-label">Image 2</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="image_2" placeholder="url">
                </div>
            </div>
            <div class="form-group">
                <label for="image_3" class="col-sm-2 control-label">Image 3</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="image_3" placeholder="url">
                </div>
            </div>
            <div class="form-group">
                <label for="video" class="col-sm-2 control-label">Video</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="video" placeholder="url">
                </div>
            </div>
            <div class="form-group">
                <label for="target-sum" class="col-sm-2 control-label">Target Sum</label>
                <div class="col-sm-10">
                    <input type="number" class="form-control" id="target-sum" placeholder="1000000">
                </div>
            </div>
            <div class="form-group">
                <label for="target-date" class="col-sm-2 control-label">Target Date</label>
                <div class="col-sm-10">
                    <input type="date" class="form-control" id="target-date" placeholder="01/01/1900">
                </div>
            </div>
            <div class="form-group">
                <label for="bank-account" class="col-sm-2 control-label">Bank Account</label>
                <div class="col-sm-10">
                    <input type="number" class="form-control" id="bank-account" placeholder="1111">
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <a class="btn btn-default" href="#" role="button" onclick="project_register();">Submit</a>
                </div>
            </div>
            <% if(err) { %>
            <p style="color: red"><%= err %></p>
            <% } %>
        </form>
    </div>
</div>